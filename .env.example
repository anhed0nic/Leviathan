# Leviathan Configuration Example
# Copy this file to .env and modify as needed

# General settings
DEBUG=false
LOG_LEVEL=INFO

# Pipeline settings
MAX_CONCURRENT_TASKS=10
TASK_TIMEOUT=300

# Module settings
# ENABLED_MODULES=discovery,detection,fuzzing,analysis,reporting

# ML settings
ML_ENABLED=true

# Database settings
DB_URL=sqlite:///leviathan.db

# Redis settings
REDIS_URL=redis://localhost:6379

# Metrics settings
METRICS_ENABLED=true
METRICS_PORT=8000